      <image />
      <image />
      <image />
    </svg>
  </svg>
  <svg id="elevation-text-container">
    <use id="elevation-legend" href="#legend">
      <set href="image" attributeName="href"
        to="images/stat_floors_solid_48px.png"/>
      <set href="image" attributeName="fill"
