  <svg id="battery-value-conainer" class="bigsarcs-container">
    <arc class="battery-arc-back back" />
    <arc id="battery-arc-back" class="front"/> 
  </svg>
  <svg id="elevation-value-conainer" class="bigsarcs-container">
    <arc class="elevation-arc-back back" />
    <arc id="elevation-arc-back" class="front"/>
